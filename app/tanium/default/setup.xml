<setup>
    <block title="Tanium API Server Credentials"></block>

    <block title="Add Server Info" endpoint="tanium/tanium_configure" entity="taniumserver">
        <input field="taniumhost">
            <label>Tanium API Server</label>
            <type>text</type>
        </input>
    </block>

<block title="Add new credentials" endpoint="storage/passwords" entity="_new">
  <input field="name">
    <label>Username</label>
    <type>text</type>
  </input>

  <input field="password">
    <label>Password</label>
    <type>password</type>
  </input>
</block>


</setup>